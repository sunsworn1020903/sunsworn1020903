local DropSection = BuyTab:CreateSection("Drop")

DropSection:CreateToggle("The banana", function(loops)
    if loops then
    _G.on = true
    local A_1 = ("The banana")
    while _G.on == true do
    
	Equip:FireServer(A_1)
	wait(0.5)
	Drop:FireServer(A_1)
	wait(0.5)
	end
else
_G.on = false
	end
end)

DropSection:CreateToggle("Pop It Rainbow!", function(loops)
