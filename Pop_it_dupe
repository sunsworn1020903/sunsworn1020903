
loadstring(game:HttpGet(("https://raw.githubusercontent.com/sunsworn1020903/sunsworn1020903/main/Pop_it_Safe_place"), true))() -- Safe 
local Pop_it_hub = loadstring(game:HttpGet("https://pastebin.com/raw/3gQQtaKX"))()
local DupeTab = Pop_it_hub:CreateTab("Dupe")
local Starter = DupeTab:CreateSection("Starter")

local Sell = "" 
local BuyItemCash = ""
local Equip = ""
local Drop = ""
local Tpwalkval = 1
local Players = Game.Players
local plr = Players.localPlayer

 function PlaceId()
if game.PlaceId == 7346416636 then
Equip = game:GetService("ReplicatedStorage").RemoteEvents.Equip
Drop = game:GetService("ReplicatedStorage").RemoteEvents.Drop
Sell = game:GetService("ReplicatedStorage").RemoteEvents.Sell
BuyItemCash = game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash
end
end
 PlaceId()

Starter:CreateToggle("Virus( 40 )", function(loops)
  if loops then
                _G.on = true
                
                while _G.on == true do
                    wait()
                
                wait(0.2)
                local args = {
                        [1] = "Virus"
                    }
                    Sell:FireServer(unpack(args))
                        local args = {
                        [1] = "Virus"
                    }
                    BuyItemCash:FireServer(unpack(args))
                
                wait(0.2)
                
                    end
                    else
                _G.on = false
                        
       end
end) 


