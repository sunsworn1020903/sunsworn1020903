 local ScriptTab = Pop_it_hub:CreateTab("Script")
 local ScriptSection = ScriptTab:CreateSection("Script")
 
 ScriptSection:CreateToggle("Anti Reaper", function(loops)
     if loops then
    local A_2 = "Watermelon"
   _G.on = true
   while _G.on == true do
   Equip:FireServer(A_2)
   wait(.2)
   end
  else
  _G.on = false
  end
 end)
 
 ScriptSection:CreateToggle("Buy, Flower Seed( 2.5k )", function(loops)
     if loops then
    local A_2 = "Flower Seed"
    _G.on = true 
    while _G.on == true do
   BuyItemCash:FireServer(A_2)
   wait(.2)
   end
    else
    _G.on = false
  end
 end)
 
 ScriptSection:CreateToggle("Buy, Watermelon( 5k )", function(loops)
     if loops then
    local A_2 = "Watermelon"
    _G.on = true 
    while _G.on == true do
   BuyItemCash:FireServer(A_2)
   wait(.2)
   end
    else
    _G.on = false
  end
 end)
 
 ScriptSection:CreateButton("XRay", function()
     game.Players.localPlayer.XRay.Value = true
 end)
 
 ScriptSection:CreateButton("Inf jump", function()
     
game:GetService("UserInputService").JumpRequest:connect(function()
	
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end)
 end)
 
ScriptSection:CreateSlider("TP walk", 1, 20, 1, false, function(value)
    Tpwalkval = value
end)

ScriptSection:CreateButton("TP walk", function(Button)
    if Button.Text == "TP walk" then
    Button.Text = "TP walking"
       local hb = game:GetService("RunService").Heartbeat
	tpwalking = true
	local chr = plr.Character
	local hum = chr and chr:FindFirstChildWhichIsA("Humanoid")
	while tpwalking and hb:Wait() and chr and hum and hum.Parent do
		if hum.MoveDirection.Magnitude > 0 then
				chr:TranslateBy(hum.MoveDirection * tonumber(Tpwalkval))
			else
				chr:TranslateBy(hum.MoveDirection)
      end
   end
    else
    Button.Text = "TP walk"
    tpwalking = false
    end
end)
 
 
 
 
 
 
