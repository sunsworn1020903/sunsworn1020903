
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/sunsworn1020903/sunsworn1020903/f29a9b87184e77890ba9bc7ffef307420be36e68/Library"))()

local Window = Library.CreateLib("sun-Gui", "Serpent")
local Tab = Window:NewTab("Servers")
local Section = Tab:NewSection("Main")
local Tab_2 = Window:NewTab("Script")
local Section_2 = Tab_2:NewSection("Script")

 local PlaceId = Game.PlaceId
 
 function NewButton(v)
     v = v or nil

  Section:NewButton("Max: "..v.maxPlayers.."     ".."Current: "..v.playing, "Nothing", function()
     Game:GetService"TeleportService":TeleportToPlaceInstance(PlaceId,v.id,game.Players.LocalPlayer)
    end)
 end

 
 for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
    
 NewButton(v)
     
 end
 
 function Refresh()
      
      for property, Instance in pairs(Game.CoreGui["sun-Gui"].Main.pages.Pages.Page.sectionFrame.sectionInners:GetChildren()) do
      if Instance:IsA("TextButton") then
        Instance:remove()
           end
        end
     wait()
     for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
        
        NewButton(v)
        
        end
     end

 Section_2:NewButton("Refresh", "Remove all Buttons and Instance new", function()
     pcall( Refresh )
 end)
 
 
 Section_2:NewTextBox("PlaceId", "To copy PlaceId just print(Game.PlaceId)", function(txt)
	PlaceId = txt
end)

 Section_2:NewButton("Back", "Game ID", function()
     PlaceId = Game.PlaceId
     wait()
     Refresh()
     end)
